        <oer:attribute description="The date on which the record is last updated." name="LastUpdateDate" />
        <oer:attribute description="The user login for the user who last updated the record." name="LastUpdateLogin" />
        <oer:attribute description="The user who last updated the record." name="LastUpdatedBy" />
        <oer:attribute description="The indicator for whether a sales account is a named sales account." name="NamedAccountFlag" />
        <oer:attribute description="The number to implement optimistic locking. This number is incremented every time that the row is updated. The number is compared at the start and end of a transaction to detect whether another session has updated the row since it was queried." name="ObjectVersionNumber" />
        <oer:attribute description="The identifier for the trading community party." name="SalesAccountPartyId" />
        <oer:attribute description="The indicator showing whether there is an existing selling or billing relationship with the sales account." name="ExistingFlag" />
        <oer:attribute description="The date when the existing flag is last modified." name="ExistingFlgLastUpdDate" />
        <oer:attribute description="The date and time of the last update from mobile." name="UserLastUpdateDate" />
        <oer:attribute description="The structure definition of the user descriptive flexfield." name="AttributeCategory" />
        <oer:attribute description="The sales party identifier." name="PartyId" />
