  <xsd:annotation>
    <xsd:documentation xmlns:oer="http://xmlns.oracle.com/oer">
      <name xmlns="http://xmlns.oracle.com/apps/cdm/foundation/parties/personService/">Trading Community Person Profile</name>
      <description xmlns="http://xmlns.oracle.com/apps/cdm/foundation/parties/personService/">A trading community person's profile information. For example
                this person is a married woman whose previous last name was
                Smith.</description>
      <oer:category name="lba" xmlns:oer="http://xmlns.oracle.com/oer">
        <value xmlns="http://xmlns.oracle.com/apps/cdm/foundation/parties/personService/">CdmTop-Customer Data Management</value>
        <value xmlns="http://xmlns.oracle.com/apps/cdm/foundation/parties/personService/">CdmTop-Customer Data Management : FoundationTop-Foundation</value>
        <value xmlns="http://xmlns.oracle.com/apps/cdm/foundation/parties/personService/">CdmTop-Customer Data Management : FoundationTop-Foundation :
                    FoundationParties-Parties</value>
      </oer:category>
      <oer:attributes xmlns:oer="http://xmlns.oracle.com/oer">
        <oer:attribute description="Unique identifier for the person profile" name="PersonProfileId" />
        <oer:attribute description="Identifier for the party. Foreign key to the HZ_PARTIES table." name="PartyId" />
        <oer:attribute description="The combination of title, first, middle, last, and suffix names." name="PersonName" />
        <oer:attribute description="Who column: indicates the date and time of the last update of the row." name="LastUpdateDate" />
        <oer:attribute description="Who column: indicates the user who last updated the row." name="LastUpdatedBy" />
        <oer:attribute description="Who column: indicates the date and time of the creation of the row." name="CreationDate" />
        <oer:attribute description="Who column: indicates the user who created the row." name="CreatedBy" />
        <oer:attribute description="Who column: indicates the session login associated to the user who last updated the row." name="LastUpdateLogin" />
        <oer:attribute description="Enterprise Service Scheduler: indicates the request ID of the job that created or last updated the row." name="RequestId" />
        <oer:attribute description="Salutary introduction such as Mr., Herr, etc." name="PersonPreNameAdjunct" />
        <oer:attribute description="First name of the person" name="PersonFirstName" />
        <oer:attribute description="Middle name of the person" name="PersonMiddleName" />
        <oer:attribute description="Last or surname of the person" name="PersonLastName" />
        <oer:attribute description="Indicates the place in a family structure. For example, in &quot;Tom Jones III&quot;, the &quot;III&quot;." name="PersonNameSuffix" />
        <oer:attribute description="A professional or family title. For example, Don or The Right Honorable." name="PersonTitle" />
        <oer:attribute description="Academic title that is part of the person's name such as Dr. Jane Smith" name="PersonAcademicTitle" />
        <oer:attribute description="Previous last or surname of the person" name="PersonPreviousLastName" />
        <oer:attribute description="Name initials" name="PersonInitials" />
        <oer:attribute description="Taxpayer identification number, often unique identifier of person or organization. Can be SSN or income taxpayer ID in US, fiscal code or NIF in Europe" name="JgzzFiscalCode" />
        <oer:attribute description="Date the person was born" name="DateOfBirth" />
        <oer:attribute description="Where the person was born. Typically, the city and country." name="PlaceOfBirth" />
        <oer:attribute description="Date the person died" name="DateOfDeath" />
        <oer:attribute description="The gender (male, female, unknown) of the person" name="Gender" />
        <oer:attribute description="The declared ethnicity of the person" name="DeclaredEthnicity" />
        <oer:attribute description="Marital status of the person" name="MaritalStatus" />
        <oer:attribute description="Date when the person's marital status changed." name="MaritalStatusEffectiveDate" />
        <oer:attribute description="The estimated gross annual income of the person" name="PersonalIncomeAmount" />
        <oer:attribute description="Indicates if the person rents or owns his or her residence. Lookup of values for rent, own, lease, and so on." name="RentOwnInd" />
        <oer:attribute description="The geophysical location (GPS point) that was last reported for the person" name="LastKnownGPS" />
        <oer:attribute description="Date Effective Entity: indicates the date at the beginning of the date range within which the row is effective." name="EffectiveStartDate" />
        <oer:attribute description="Date Effective Entity: indicates the date at the end of the date range within which the row is effective." name="EffectiveEndDate" />
        <oer:attribute description="Indicates whether the person is internal or external to the user's organization. Y for a person who is a member of the user's organization, N for a person who is not a member of the user's organization." name="InternalFlag" />
        <oer:attribute description="Record status flag" name="Status" />
        <oer:attribute description="TCA Who column" name="CreatedByModule" />
        <oer:attribute description="Y or N indicates whether the person is deceased or not" name="DeceasedFlag" />
        <oer:attribute description="Description for a Person." name="Comments" />
        <oer:attribute description="Used to capture the Last name prefix (e.g. 'fon', 'van', etc.) for a person. It is used in the same way the new field PERSON_LAST_NAME_PREFIX in HZ_PERSON_PROFILES is used. Corresponds to the field Last Name Prefix in HL7." name="PersonLastNamePrefix" />
        <oer:attribute description="Used to capture the second last name for a person. In some cultures it is not uncommon to have double last name. It is used in the same way the new field PERSON_SECOND_LAST_NAME in HZ_PERSON_PROFILES is used." name="PersonSecondLastName" />
        <oer:attribute description="Default Currency code for this person." name="PreferredFunctionalCurrency" />
        <oer:attribute description="Source system name" name="OrigSystem" />
        <oer:attribute description="Unique person identifier from foreign system" name="OrigSystemReference" />
        <oer:attribute description="Date Effective Entity: indicates the order of different changes made during a day. The lowest value represents the earliest change in the day." name="EffectiveSequence" />
        <oer:attribute description="Indicates if the person is the head of the household. Y for the head of a household, N for people who are not the head of a household." name="HeadOfHouseholdFlag" />
        <oer:attribute description="Income of the household that this person is a part of" name="HouseholdIncomeAmount" />
        <oer:attribute description="The size of the household this person is a part of" name="HouseholdSize" />
        <oer:attribute description="Date Effective Entity: 'Y' indicates that this row represents the latest change in the day." name="EffectiveLatestChange" />
        <oer:attribute description="This attribute will have Y if the suffix is overridden by user and N if not. Default value is N." name="SuffixOverriddenFlag" />
        <oer:attribute description="Captures system generated or manually overridden suffix." name="UniqueNameSuffix" />
        <oer:attribute description="Corporate Currency Code This attribute is used by CRM Extensibility framework" name="CorpCurrencyCode" />
        <oer:attribute description="Currency Conversion Rate Type This attribute is used by CRM Extensibility framework" name="CurcyConvRateType" />
        <oer:attribute description="Currency Code This attribute is used by CRM Extensibility framework" name="CurrencyCode" />
        <oer:attribute description="Person Information." name="PersonInformation" />
      </oer:attributes>
    </xsd:documentation>
  </xsd:annotation>
